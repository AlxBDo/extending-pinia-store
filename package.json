{
  "author": "Alexandre Bidaud <alexbidaud85@gmail.com> (https://github.com/AlxBDo)",
  "name": "extending-pinia-store",
  "description": "A Pinia plugin to create reusable stores that can extend other Pinia stores.",
  "version": "0.0.0-alpha.1",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/extending-pinia-store.js",
      "require": "./dist/extending-pinia-store.umd.cjs"
    }
  },
  "main": "./dist/extending-pinia-store.umd.cjs",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly --outDir dist",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "pinia": "^3.0.2",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vue": "^3.5.24",
    "vue-tsc": "^3.1.4"
  },
  "dependencies": {
    "pinia-plugin-subscription": "^0.0.0-alpha.4"
  }
}
